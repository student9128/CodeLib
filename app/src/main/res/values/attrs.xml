<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="HexagonProgressBar">
        <attr name="hexagonColor" format="color" />
        <attr name="hexagonProgressColor" format="color" />
        <attr name="hexagonWidth" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textIsDisplayable" format="boolean" />
        <attr name="contentColor" format="color" />
        <attr name="text" format="string" />
        <attr name="progress" format="integer" />
    </declare-styleable>
    <declare-styleable name="ToggleView">
        <attr name="toggleColor" format="color" />
        <attr name="checkedColor" format="color" />
        <attr name="unCheckedColor" format="color" />
        <attr name="checked" format="boolean" />
        <attr name="borderWidth" format="dimension" />
    </declare-styleable>
</resources>